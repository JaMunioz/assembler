DATA:
  lista1 #A0
  lista2 #A8
  initial #B0
  mult 0
  res 0
  counter 0
  aux1 0
  dir1 #A0
  dir2 #A8
  final 0
CODE:
  MOV A,counter
  CMP A,2
  JEQ FIN
  JNE LISTAS
LISTAS:
  INC (counter)
  MOV A,dir1
  MOV B,counter
  ADD B,A
  MOV A,(B)
  MOV (mult),A
  MOV A,dir2
  MOV B,counter
  ADD B,A
  MOV A,(B)
VOLVERASUMAR:
  MOV A,(B)
  MOV B,(mult)
  ADD B,(mult)
  MOV (mult),B
  MOV B,(aux1)
  CMP A,B
  JEQ CONTINUAR
  CMP A,B
  JNE VOLVERASUMAR
CONTINUAR:
  MOV A,(res)
  MOV B,(mult)
  ADD B,A
  MOV (res),B
  RST (mult)
  JMP LISTAS
FIN:
  MOV A,(res)
  MOV (final),A
